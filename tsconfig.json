{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"],
      "hono": ["./node_modules/@hono/react"]
    },
    "types": [
      "node"
    ],
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
